{
  "extends": ["@tsconfig/esm/tsconfig.json", "@tsconfig/strictest/tsconfig.json"],
  "exclude": ["dist", "server"],
  "compilerOptions": {
    "noEmit": true,
    "incremental": true,
    "types": ["vite/client", "@cloudflare/workers-types"],
    "lib": ["esnext", "dom", "dom.iterable", "webworker"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "paths": {
      "~/vite": ["./vite.config.ts"],
      "~/*": ["./src/*"]
    }
  }
}
